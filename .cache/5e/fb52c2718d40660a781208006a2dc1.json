ems: center;\n  justify-content: center;\n  border: 1px solid rgba(0, 0, 0, 0.2);\n  box-sizing: border-box;\n  min-height: 60px;\n  min-width: 60px;\n}\n\n.water {\n  background-color: #424fdb;\n  opacity: 1;\n  background-image: repeating-radial-gradient(\n      circle at 0 0,\n      transparent 0,\n      #424fdb 10px\n    ),\n    repeating-linear-gradient(#262a7855, #262a78);\n}\n\n.dirt {\n  background-color: #b67437;\n  opacity: 1;\n  background-image: radial-gradient(#784e26 0.75px, transparent 0.75px),\n    radial-gradient(#34631e 0.75px, #61972d 0.75px);\n  background-size: 11px 14px;\n  background-position: 0 0, 19px 21px;\n}\n\n/* Tooltip to display when hovering over animals */\n.tooltip {\n  cursor: pointer;\n  position: relative;\n  white-space: pre-wrap;\n}\n.left.tooltip:before {\n  left: unset;\n  top: 100%;\n  right: 100%;\n  transform: unset;\n}\n.tooltip:before {\n  z-index: 999;\n  box-sizing: border-box;\n  content: attr(data-text);\n  position: absolute;\n  top: 50%;\n  transform: translateY(-50%);\n  left: 100%;\n  margin-left: 15px;\n\n  font-size: 10;\n  line-height: 3;\n  width: 200px;\n  padding: 10px;\n  border-radius: 10px;\n  background: #000;\n  color: #fff;\n\n  display: none; /* hide by default */\n}\n\n.tooltip:hover:before {\n  display: block;\n}\n"},"lineCount":null}},"error":null,"hash":"172508ab0ee44815b45a5b527f8b6491","cacheData":{"env":{}}}